{
  "name": "moltinql",
  "private": true,
  "version": "0.0.3",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -e ts,graphql -x ts-node -r dotenv/config src",
    "deploy":
      "now --public --dotenv .env.production && now alias && now rm --yes --safe moltinql",
    "build": "rimraf dist && tsc",
    "start": "node dist"
  },
  "dependencies": {
    "@moltin/sdk": "^3.3.0",
    "graphql-yoga": "^1.2.1",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "@types/node": "^9.4.0",
    "@types/node-fetch": "^1.6.7",
    "dotenv": "^5.0.0",
    "nodemon": "^1.14.12",
    "now": "^9.2.7",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "now": {
    "alias": "moltinql"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
